<script type="text/javascript">
function showdiv(id){
	for(i=1;i<=11;i++){
		if(i==id){
			if(document.getElementById('tab_'+i)){
				document.getElementById('con_'+i).style.display='';
				document.getElementById('tab_'+i).className='on';
			}
		}else{
			if(document.getElementById('tab_'+i)){
				document.getElementById('con_'+i).style.display='none';
				document.getElementById('tab_'+i).className='off';
			}
		}
	}
}
</script>
