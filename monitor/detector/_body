<script type="text/javascript">
  function chat() {  
    $(".fixed-bottom").stop(true,true).animate({"margin-left":"0px"},500);
    $(".cai-like").css("display","none"); 
    $(".fixed-bottom").css("background","#f3f3f3");
  } 
  $(function(){ 
   //猜你喜欢
    setInterval(chat, "30000"); 
    $(".fixed-bottom").stop(true,true).animate({"margin-left":"0px"},500); 
    $(".turnoff").click(function(){
    $(".fixed-bottom").stop(true,true).animate({"margin-left":"-2000px"},500);
    $(".cai-like").css("display","block");
    $(".fixed-bottom").css("background","none");
  })
   $(".cai-like").click(function(){
    $(".fixed-bottom").stop(true,true).animate({"margin-left":"0px"},500); 
    $(".cai-like").css("display","none");
    $(".fixed-bottom").css("background","#f3f3f3");
   })
  })
